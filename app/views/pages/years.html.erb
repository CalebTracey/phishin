<%= render 'index_header' %>

<div id="content">
      
  <h1>3.0 Era</h1>
  <div id="era3">
    <ul class="year_list">
      <% 2012.downto(2009) do |year|  %>
        <li><%= link_to year.to_s, "/" + year.to_s %></li>
      <% end %>
    </ul>
    <div style="clear: both;"></div>
  </div>
      
  <h1>2.0 Era</h2>
  <div id="era2">
    <ul class="year_list">
      <% 2004.downto(2002) do |year|  %>
        <li><%= link_to year.to_s, "/" + year.to_s %></li>
      <% end %>
    </ul>
    <div style="clear: both;"></div>
  </div>
      
  <h1>1.0 Era</h1>
  <div id="era1">
    <ul class="year_list">
      <% 2000.downto(1988) do |year|  %>
        <li><%= link_to year.to_s, "/" + year.to_s %></li>
      <% end %>
      <li><%= link_to '1987 - 83', '/1987-83' %></li>
    </ul>
    <div style="clear: both;"></div>
  </div>
      
  <div style="clear: both;"></div>
      
</div>